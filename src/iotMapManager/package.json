{
  "name": "iotmapmanager",
  "version": "2.6.10-alpha.1",
  "description": "Manage markers, clusters, user marker or paths on IotMaps",
  "main": "dist/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "start": "webpack-dev-server --mode development --open",
    "build": "webpack --mode development",
    "dist": "webpack --mode production",
    "release": "npm run dist && npm pack",
    "watch": "webpack --watch --mode development"
  },
  "files": [
    "dist/**/*",
    "lib/**/*",
    "css/*",
    "img/*",
    "iot-map-manager.css",
    "package.json",
    "readme.md",
    "LICENSE",
    "NOTICE.txt"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Orange-OpenSource/IOT-Map-Component.git"
  },
  "keywords": [
    "IotMap",
    "Markers",
    "Clusters",
    "UserMarker",
    "Path"
  ],
  "author": "Orange SA",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Orange-OpenSource/IOT-Map-Component/issues"
  },
  "homepage": "https://github.com/Orange-OpenSource/IOT-Map-Component#readme",
  "peerDependencies": {
    "leaflet": ">=1.6.0",
    "leaflet-polylineoffset": ">=1.1.1",
    "leaflet.markercluster": ">=1.4.1"
  },
  "devDependencies": {
    "@types/leaflet": "^1.9.21",
    "@types/leaflet.markercluster": "^1.5.5",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  }
}
